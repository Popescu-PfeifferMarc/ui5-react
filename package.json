{
	"name": "ui5-react-test",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"format": "prettier --config package.json --write .",
		"lint": "eslint .",
		"lint:fix": "eslint --fix ."
	},
	"dependencies": {
		"@tanstack/react-query": "^4.15.1",
		"@tanstack/react-query-devtools": "4.3.0-beta.7",
		"@ui5/webcomponents": "~1.9.1",
		"@ui5/webcomponents-base": "^1.9.1",
		"@ui5/webcomponents-fiori": "~1.9.1",
		"@ui5/webcomponents-icons": "~1.9.1",
		"@ui5/webcomponents-react": "^1.4.1",
		"@ui5/webcomponents-react-base": "^1.4.0",
		"localforage": "^1.10.0",
		"match-sorter": "^6.3.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-router-dom": "^6.4.3",
		"sort-by": "^1.2.0",
		"web-vitals": "^3.0.4"
	},
	"devDependencies": {
		"@types/node": "^18.11.9",
		"@types/react": "^18.0.25",
		"@types/react-dom": "^18.0.8",
		"@typescript-eslint/eslint-plugin": "^5.42.1",
		"@vitejs/plugin-react": "^2.2.0",
		"eslint": "^8.27.0",
		"eslint-config-airbnb": "^19.0.4",
		"prettier": "^2.7.1",
		"typescript": "^4.8.4",
		"vite": "^3.2.3"
	},
	"eslintConfig": {
		"env": {
			"browser": true,
			"es2021": true,
			"node": true
		},
		"extends": [
			"eslint:recommended",
			"plugin:react/recommended",
			"plugin:@typescript-eslint/recommended",
			"airbnb",
			"airbnb/hooks"
		],
		"overrides": [],
		"parser": "@typescript-eslint/parser",
		"parserOptions": {
			"ecmaVersion": "latest",
			"sourceType": "module"
		},
		"plugins": [
			"react",
			"@typescript-eslint"
		],
		"rules": {
			"no-tabs": "off",
			"react/jsx-indent": "off",
			"react/jsx-filename-extension": "off",
			"indent": "off",
			"import/extensions": "off",
			"no-unused-vars": "warn",
			"object-curly-newline": "off",
			"import/no-unresolved": "off",
			"import/no-extraneous-dependencies": "off",
			"implicit-arrow-linebreak": "off",
			"function-paren-newline": "off",
			"no-console": "off",
			"react/function-component-definition": "off",
			"react/jsx-one-expression-per-line": "off",
			"arrow-body-style": "off",
			"react/jsx-indent-props": "off",
			"operator-linebreak": "off",
			"no-mixed-spaces-and-tabs": "off"
		},
		"ignorePatterns": [
			"node_modules",
			"dist"
		]
	},
	"prettier": {
		"semi": true,
		"trailingComma": "all",
		"singleQuote": true,
		"printWidth": 110,
		"tabWidth": 4,
		"useTabs": true
	}
}
