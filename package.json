{
	"name": "ui5-react-test",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"format": "prettier --config package.json --write .",
		"lint": "eslint .",
		"lint:fix": "eslint --fix .",
		"backend": "rimraf src/server/ && orval --config orval.config.ts",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@tanstack/react-query": "^4.21.0",
		"@tanstack/react-query-devtools": "4.21.0",
		"@tanstack/react-table": "^8.7.4",
		"@ui5/webcomponents": "~1.9.3",
		"@ui5/webcomponents-base": "^1.9.3",
		"@ui5/webcomponents-fiori": "~1.9.3",
		"@ui5/webcomponents-icons": "~1.9.3",
		"@ui5/webcomponents-react": "^1.7.1",
		"@ui5/webcomponents-react-base": "^1.7.0",
		"axios": "^1.2.2",
		"orval": "^6.11.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-router-dom": "^6.6.1",
		"rimraf": "^3.0.2",
		"web-vitals": "^3.1.0"
	},
	"devDependencies": {
		"@types/node": "^18.11.18",
		"@types/react": "^18.0.26",
		"@types/react-dom": "^18.0.10",
		"@typescript-eslint/eslint-plugin": "^5.48.0",
		"@vitejs/plugin-react": "^3.0.1",
		"eslint": "^8.31.0",
		"eslint-config-airbnb": "^19.0.4",
		"prettier": "^2.8.2",
		"typescript": "^4.9.4",
		"vite": "^4.0.4"
	},
	"eslintConfig": {
		"env": {
			"browser": true,
			"es2021": true,
			"node": true
		},
		"extends": [
			"eslint:recommended",
			"plugin:react/recommended",
			"plugin:@typescript-eslint/recommended",
			"airbnb",
			"airbnb/hooks"
		],
		"overrides": [],
		"parser": "@typescript-eslint/parser",
		"parserOptions": {
			"ecmaVersion": "latest",
			"sourceType": "module"
		},
		"plugins": [
			"react",
			"@typescript-eslint"
		],
		"rules": {
			"arrow-body-style": "off",
			"function-paren-newline": "off",
			"implicit-arrow-linebreak": "off",
			"indent": "off",
			"no-mixed-spaces-and-tabs": "off",
			"no-tabs": "off",
			"object-curly-newline": "off",
			"operator-linebreak": "off",
			"react/jsx-indent": "off",
			"react/jsx-filename-extension": "off",
			"react/jsx-wrap-multilines": "off",
			"react/function-component-definition": "off",
			"react/jsx-one-expression-per-line": "off",
			"react/jsx-indent-props": "off",
			"import/extensions": "off",
			"import/no-unresolved": "off",
			"import/no-extraneous-dependencies": "off",
			"no-console": "off",
			"no-unused-vars": "warn",
			"max-len": "off",
			"import/prefer-default-export": "off",
			"react/react-in-jsx-scope": "off",
			"react/destructuring-assignment": "off"
		},
		"ignorePatterns": [
			"node_modules",
			"dist"
		]
	},
	"prettier": {
		"semi": true,
		"trailingComma": "all",
		"singleQuote": true,
		"printWidth": 110,
		"tabWidth": 4,
		"useTabs": true
	}
}
